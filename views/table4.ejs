<h3>Список сотрудников</h3>
<div class="table-responsive">
    <table class="table table-bordered table-striped table-hover">
        <thead>
            <tr>
                <th>ID</th>
                <th>Фамилия</th>
                <th>Имя</th>
                <th>Отчество</th>
                <th>Должность</th>
            </tr>
        </thead>
        <tbody>
            <% data.forEach(user=> { %>
                <tr>
                    <td>
                        <%= user.ID %>
                    </td>
                    <td>
                        <%= user.SURNAME %>
                    </td>
                    <td>
                        <%= user.NAME %>
                    </td>
                    <td>
                        <%= user.PATRONYMIC %>
                    </td>
                    <td>
                        <%= user.POSITION %>
                    </td>
                </tr>
                <% }) %>
        </tbody>
    </table>
</div>